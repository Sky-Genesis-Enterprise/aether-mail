{
  "displayName": "Aether Mail OS",
  "license": "AGPL-3.0 license",
  "description": "Aether Mail OS is a modern, open-source email client built with React, Vite, and Tailwind CSS. It offers a sleek and intuitive interface for managing your emails efficiently.",
  "author": "Sky Genesis Enterprise",
  "email": "contact@skygenesisenterprise.com",
  "name": "react-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "pr": "node scripts/pr.mjs",
    "dev": "vite --host 0.0.0.0",
    "dev:frontend": "vite --host 0.0.0.0",
    "dev:backend": "nodemon",
    "test": "jest --watchAll --passWithNoTests",
    "test:frontend": "jest --watchAll --passWithNoTests",
    "test:backend": "jest --watchAll --passWithNoTests",
    "test:unit": "jest --watchAll --passWithNoTests",
    "test:integration": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:coverage:frontend": "jest --coverage --passWithNoTests",
    "test:coverage:backend": "jest --coverage --passWithNoTests",
    "test:coverage:unit": "jest --coverage --passWithNoTests",
    "build": "tsc -b && vite build --outDir dist",
    "build:frontend": "vite build --outDir dist/frontend",
    "build:backend": "tsc -p tsconfig.server.json",
    "lint": "bunx biome lint --write && bunx tsc --noEmit",
    "format": "bunx biome format --write",
    "preview": "vite preview",
    "start:backend": "nodemon --watch src --ext ts,json --exec ts-node-esm ./src/api/server.ts",
    "start:frontend": "vite preview",
    "start": "npm run start:backend & npm run start:frontend",
    "electron": "electron ./electron/main.cjs",
    "docker:build": "docker build -t aether-mail-os .",
    "docker:run": "docker run -p 5000:5000 aether-mail-os",
    "docker:compose:up": "docker-compose up",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.6.5",
    "inquirer": "^8.2.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.487.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.18.0",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-validator": "^3.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^35.2.0",
    "eletron": "^0.0.1-security",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "express": "^5.1.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.4"
  }
}
